### HIGH-LEVEL EVENTS
#	Think DVD-Chapters
#	Agent, Factor, Location inferred from sub_events

:adipose_industries_exposition a event:Event;
	rdfs:label "The Doctor and Donna both investigate Adipose Industries";
	event:sub_event :building_entry;
	event:sub_event :presentation;
	event:sub_event :cubicles;
	event:sub_event :doctor_and_roger.
	
:parthenogenesis_single a event:Event;
	rdfs:label "Donna visits Stacey, an Adipose Industries Customer. Something dramatic happens to Stacey";
	event:sub_event :stacey_and_donna;
	event:sub_event :partho_activated;
	event:sub_event :stacey_partho;
	event:sub_event :donna_investigates.

:parthenogenesis_investigations a event:Event;
	rdfs:label "Stacey's parthenogenesis is investigated by all parties";
	event:sub_event :doctor_and_roger;
	event:sub_event :collection_squad_sent;
	event:sub_event :doctor_investigates;
	event:sub_event :collection_squad_collects;
	event:sub_event :foster_investigates;
	event:sub_event :donna_with_family;
	event:sub_event :doctor_investigates_pendant;
	event:sub_event :doctor_to_storeroom;
	event:sub_event :donna_to_cubicles;
	event:sub_event :donna_cubicles;
	event:sub_event :foster_investigates_cubicles;
	event:sub_event :noble_family_prepare_for_evening;
	event:sub_event :doctor_storeroom;
	event:sub_event :doctor_snooping;
	event:sub_event :pennys_capture.
	
:team_doctor_and_donna a event:Event;
	rdfs:label "On the trail of Miss Foster, the Doctor and Donna meet and escape capture";
	event:sub_event :doctor_donna_meet;
	event:sub_event :doctor_donna_escape;
	event:sub_event :fosters_chase;
	event:sub_event :doctor_rescues_donna;
	event:sub_event :penny_released;
	event:sub_event :foster_and_doctor_reveal_indentities;
	event:sub_event :premature_labour_triggered;
	event:sub_event :penny_recaptured;
	event:sub_event :foster_acts;
	event:sub_event :doctor_hacking;
	event:sub_event :guards_stunned.

:parthenogenesis_all a event:Event;
	rdfs:label "Miss Foster brings forward her plans: full parthenogenesis, now";
	event:sub_event :parthenogenesis;
	event:sub_event :adipose_collection;
	event:sub_event :foster_acts2;
	event:sub_event :doctor_hacking2;
	event:sub_event :donna_produces_capsule.
	
:adipose_arrive a event:Event;
	rdfs:label "The Adiposians arrive, collect their young, and dispose of Miss Foster";
	event:sub_event :nursery_arrives;
	event:sub_event :adiposians_communicate;
	event:sub_event :adipose_collection;
	event:sub_event :fosters_collection.
	
:donna_is_new_assistant a event:Event;
	rdfs:label "Action over, Donna leaves with Doctor as new assistant";
	event:sub_event :wilf_night;
	event:sub_event :nursery_leaves;
	event:sub_event :doctor_reports_crime;
	event:sub_event :penny_finally_escapes;
	event:sub_event :assistant_discussion;
	event:sub_event :departure.


